{
  "name": "uinames",
  "description": "A simple module to generate random localized names using uinames(http://uinames.com) as the source",
  "version": "0.0.3",
  "dependencies": {    
    "defaults": "^1.0.0",
    "request": "^2.51.0"
  },
  "scripts": {
    "lint": "jshint .",
